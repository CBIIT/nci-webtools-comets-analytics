{
    "server": {
        "port": 8000,
        "sessionFolder": "temp"
    },
    "logs": {
        "folder": "logs",
        "level": "debug"
    },
    "aws": {
        "region": "region",
        "accessKeyId": "accessKeyId",
        "secretAccessKey": "secretAccessKey"
    },
